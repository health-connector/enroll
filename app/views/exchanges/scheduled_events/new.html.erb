<div class="content landing">
  <%= render "exchanges/scheduled_events/primary_nav", :active_tab => "home-tab" %>
  
  <div class="home">
    <%= render 'shared/tab_panel' %>
  </div>
</div>
<div id="create_event">
  <div class="container top-pd">
    <div class="tab-wrapper">
      <h3 class="title-inline">Create Scheduled Events</h3>
    </div>
  </div>
  <div id="schedules_event_form">
    <%= form_for @scheduled_event, as: :scheduled_event, url: {action: "create"} do |f| %>
      <%= render "event_form", f: f %>
    <% end %>
  </div>

</div>