<div class="content landing">
  <%= render "exchanges/scheduled_events/primary_nav", :active_tab => "home-tab" %>
  <div class="home">
    <%= render 'shared/tab_panel' %>
  </div>
</div>
<div class="container content-bottom">
  <div class='container'>
    <div class="row">
      <h3 class="title-inline">List Of Custom and System Events</h3>
    </div>
    <ul>
      <% scheduled_events.each do |event| %>
        <% if event.type != 'federal' %>
          <li>
            <%= link_to event.event_name.humanize, edit_exchanges_scheduled_event_path(id: event.id), class: 'btn btn-normal'%>
           </li>
        <% end %>
      <% end %>
    </ul>
  </div>
</div>
