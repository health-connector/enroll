<style>
  .ui-autocomplete {
    z-index: 9999;
  }
</style>

<div class="employee-upload">
  <i class="fa fa-times fa-3x pull-right modal-close"></i>

  <div class="upload-preview">

  </div>

  <%= form_tag(upload_v2_xml_exchanges_employer_applications_path(id: @application.id, employer_id: @benefit_sponsorship.id, employer_application_id: @application.id), id: "upload-form", multipart: true , method: :post, remote: true) do  %>
    <%= file_field_tag :file, :accept => 'application/xml' %>
    <%= hidden_field_tag :employer_actions_id, params[:employers_action_id] %>
    <%= label_tag :file, 'Select File To Upload', class: 'select btn btn-primary ' %>
    <%= submit_tag 'Upload', class: 'btn btn-primary ' %>
  <% end %>
</div>
