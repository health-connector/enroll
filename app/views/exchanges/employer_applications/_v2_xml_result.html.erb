<div class="container bottom-pd text-center">
  <% if @error_message.present? %>
    <h4 class="title" style="color:red"><%= @error_message %> </h4>
  <%end%>

  
  <h4 class="title" id="error_message" style="color:red;display:none"></h4>

  <h4 class="title" id="success_message" style="display:none"></h4>


  <% if @success_message.present? %>
    <h4 class="title"><%= @success_message %> </h4>
  <%end%>

  <table class="table">
    <thead> </thead>
    <tbody>
      <tr>
        <td> <span class="btn btn-default" onclick="$('tr.child-row:visible').remove();"> <%= l10n(".cancel") %> </span> </td>
      </tr>
    </tbody>
  </table>
</div>
