<div class="modal" id="reviseEndDateBenefitApplication" tabindex="-1" role="dialog" role="document">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title"><%= l10n("hbx_profiles.confirm")%></h4>
      </div>

      <%= form_tag(revise_end_date_exchanges_employer_applications_path, method: :put) do %>
          <div class="modal-body">
            <h4><%= l10n("exchange.employer_applications.revise_end_date.confirmation_message")%></h4>
          </div>
          <%= hidden_field_tag 'transmit_to_carrier' %>
          <%= hidden_field_tag 'employer_id' %>
          <%= hidden_field_tag 'employer_application_id' %>
          <%= hidden_field_tag 'revise_end_date' %>
          <div class="modal-footer">
            <span class="btn btn-default btn-small" data-dismiss="modal"><%= l10n("exchange.employer_applications.revise_end_date.cancel")%></span>
            <%= submit_tag l10n("exchange.employer_applications.revise_end_date.plan_year"), class: "btn btn-primary mtz  btn-small" %>
          </div>
      <% end %>
    </div>
  </div>
</div>
