<% case @carrier_search_level %>
<% when 'single_plan' %>
  $('.single-plan-carrier-container').html("<%= escape_javascript(render partial: 'employers/plan_years/carriers_list', locals: { carrier_names: @carrier_names, search_level: @carrier_search_level, benefit_group_id: @benefit_group, start_on: @start_on, location_id: @location_id }) %>");
<% when 'sole_source' %>
  $('.sole-source-carrier-container').html("<%= escape_javascript(render partial: 'employers/plan_years/carriers_list', locals: { carrier_names: @carrier_names, search_level: @carrier_search_level, benefit_group_id: @benefit_group, start_on: @start_on, location_id: @location_id }) %>");
<% when 'single_carrier' %>
  $('.single-carrier-carrier-container').html("<%= escape_javascript(render partial: 'employers/plan_years/carriers_list', locals: { carrier_names: @carrier_names, search_level: @carrier_search_level, benefit_group_id: @benefit_group, start_on: @start_on, location_id: @location_id }) %>");
<% end %>
