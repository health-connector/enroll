<div>
<div class="enrolled">
  <h3 style="margin: 5px 0;"><%= l10n('enrollment_report') %> <%= @billing_date.beginning_of_month %> - <%= @billing_date.end_of_month %></h3>
  <div class="text-danger"><%= l10n('employers.premium_statements.not_invoice') %></div>
  <%= button_to l10n('download'), employers_premium_statement_path(@employer_profile, format: :csv), method: :get, params: { billing_date: @billing_date.strftime("%m/%d/%Y")}, class: 'btn btn-default buttons-csv buttons-html5 interaction-click-control-csv' %>
    </div>
  </div>
</div>

  <% if @datatable.present? %>

  <%= raw render_datatable(@datatable,{ sDom: "<'row'<'col-sm-5'> <'col-sm-5'f> >" + "<'row'<'col-sm-10'tr>>"+ "<'row'<'col-sm-9'i> <'col-sm-3'l>>"+ "<'row'<'col-sm-10'p>>", autowidth: true,buttons: ['csv','excel'],"lengthMenu": [[10, 25, 50, -1], [10, 25, 50, "All"]]})  %>
  <% else %>
    <div class="col-xs-4"><%= l10n('no_ee_enrolled' )%></div>
  <% end %>
</div>
</div>
