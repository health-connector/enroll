<footer class="footer">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-sm-6 col-md-6">
          <p><%= copyright_notice %></p>
      </div>
      <div class="col-xs-12 col-sm-6 col-md-6">
        <ul class="list-unstyled list-inline pull-right">
          <li>
            <a class="call-customer-service" href="tel:<%= contact_center_phone_number %>">
              <span>
                <i class="fa fa-phone fa-lg"></i> &nbsp;<%= contact_center_phone_number %>
              </span>
            </a>
          </li>
          <li>
            <i class="fa fa-envelope-o fa-lg" aria-hidden="true"></i> &nbsp;<%= mail_to contact_center_email_address %>
          </li>
          <li>
            <i class="fa fa-question fa-lg" aria-hidden="true"></i> &nbsp;FAQs
          </li>
        </ul>
      </div>
    </div>
</footer>
